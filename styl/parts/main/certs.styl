#certificates
    padding 40px 0 31px
    background-image url(../img/paper_bg.png)
    background-repeat: repeat-x
    background-size contain
    position relative
    top: -85px
    z-index 9
.owl-theme .owl-nav [class*="owl-"]
    border-radius: 50%;
    background-color: rgb(222, 156, 8);
    width: 45px;
    height: 45px;
    line-height: 45px;
    color #1a140d
    font-size: 24px
    text-align: center
    float left
    margin 0
    padding: 0
    &:last-child
        float: right
    &:hover
        background-color: rgba(222, 156, 8, .6);
        color #1a140d
.owl-theme .owl-nav
    position: absolute
    top: 40%;
    height: 0;
    margin: 0 auto;
    left: -25px;
#certificates_container.owl-carousel            .owl-item img
        width: auto;
        height: 154px;
        margin: 0 auto;
        border-radius: 3px
    .owl-nav
        width: 103.3%;
.button_partner
    width: 310px
    margin:-10px auto 70px