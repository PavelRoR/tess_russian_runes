*
	margin 0
	padding 0
	outline none
body
	font-family: "OpenSansRegular", sans-serif
	background-color: #f2eddd
a
	color: #fefefe
	&:focus
		outline: 0
button
	border none
.video_wrapper
	margin 0 auto
	position relative
	&:before
		content ''
		height 0
		padding-top 56.25%
		display block
	&:after
		content: ''
		display block
		position: absolute
		top: 0
		left 0
		right 0
		width: 100%
		height 70px
	img,
	iframe
		position absolute
		width 100%
		height 100%
		display block
		background-color #fff
		border-radius 3px
		border: 0
		top 0
		left 0
		right 0
		bottom 0
		cursor pointer
.button
	display block
	border-radius: 5px;
	box-shadow: 0 0 6% 5px rgba(#ffffbe, .35) inset
	width: 260px;
	height: 60px;
	line-height: 56px;
	border: 1px solid
	font-size: 18px;
	font-family: "OpenSansBold", sans-serif;
	cursor pointer
	margin 0 auto
	text-align: center
	text-decoration: none
.button_mail
	color: rgb(30, 32, 43);
	border-color: rgb(157, 107, 5);
	background-image: linear-gradient( 180deg, #d58d03, #edb712 100%);
	&:focus
		background-image: linear-gradient(180deg, #d58d03, #edb712 100%);
		color: rgb(30, 32, 43);
		text-decoration: none
	&:hover
		background-image: linear-gradient( 0deg, #d58d03, #edb712 100%);
		color: rgb(30, 32, 43);
		text-decoration: none
.button_vk
	border-color: rgb(0, 71, 93);
	background-image: linear-gradient(180deg,  #4d76a1 0%, #355b82 100%);
	color: rgb(255, 255, 255);
	&:focus
		background-image: linear-gradient(180deg,  #4d76a1 0%, #355b82 100%);
		color: rgb(255, 255, 255);
		text-decoration: none
	&:hover
		background-image: linear-gradient(0deg,  #4d76a1 0%, #355b82 100%);
		color: rgb(255, 255, 255);
		text-decoration: none
h1, h2, h3, h4, h5, h6
	font-family: "Izhitsa", arial;

h3
	font-size: 40px;
	color: rgb(26, 20, 13);
	line-height: 0.7;
	text-align: center;
	margin: 0 0 75px
	letter-spacing: -0.6px;

